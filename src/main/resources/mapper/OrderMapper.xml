        <result column="seat_info" property="seatInfo" />
        <result column="ticket_num" property="ticketNum" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, create_time, update_time, user_id, process_id, seat_info, ticket_num
    </sql>

</mapper>
